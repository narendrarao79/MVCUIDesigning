@model IEnumerable<MVCUIDesigning.Models.Customer>
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>DisplayCustomers</title>
</head>
<body>
    <div>
        <h2>Customer Details</h2>
        @foreach (MVCUIDesigning.Models.Customer customer in Model)
        {
            Html.RenderPartial("_CustomerView", customer);
        }
    </div>
</body>
</html>
